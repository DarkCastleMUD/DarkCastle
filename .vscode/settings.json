{
    "files.associations": {
        "${workspaceFolder}/src/include/DC/*.h": "cpp",
        "set": "cpp",
        "fstream": "cpp",
        "complex": "cpp",
        "string": "cpp",
        "cmath": "cpp",
        "cstddef": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstdint": "cpp",
        "list": "cpp",
        "map": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "expected": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp",
        "variant": "cpp",
        "ctime": "cpp",
        "__node_handle": "cpp",
        "__tree": "cpp",
        "__verbose_abort": "cpp"
    },
    "editor.renderWhitespace": "all",
    "C_Cpp.default.cppStandard": "c++23",
    "C_Cpp.default.cStandard": "c23",
    "C_Cpp.codeAnalysis.clangTidy.args": [
        "-extra-arg=-std=c++23",
        "-extra-arg=-stdlib=libc++"
    ],
    "cmake.sourceDirectory": "${workspaceFolder}/src",
    "C_Cpp.codeAnalysis.exclude": {
        "**/.github": true,
        "**/.git": true,
        "**/.vscode": true,
        "**/archive": true,
        "**/blog": true,
        "**/bsave": true,
        "**/build": true,
        "**/docs": true,
        "**/familiar": true,
        "**/follower": true,
        "**/lib": true,
        "**/log": true,
        "**/public_html": true,
        "**/save": true,
        "**/tmp": true,
        "**/vaults": true,
        "**/.*": true,
        "**/*.md": true,
        "**/*.yml": true
    },
    "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
        "clang-analyzer-core.NullDereference"
    ],
    "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
        "clang-analyzer-cplusplus.*",
        "clang-analyzer-deadcode.DeadStores",
        "clang-analyzer-nullablity.*",
        "clang-analyzer-optin.*",
        "clang-analyzer-osx.*",
        "clang-analyzer-security.*",
        "clang-analyzer-unix.*",
        "clang-analyzer-valist.*"
    ],
    "editor.formatOnSave": true,
    "gitlens.telemetry.enabled": false,
    "C_Cpp.refactoring.includeHeader": "never",
    "cmake.configureOnOpen": false,
    "debug.showVariableTypes": true,
}